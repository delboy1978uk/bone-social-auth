{
    "name": "delboy1978uk/bone-social-auth",
    "description": "SocialAuth package for Bone Framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Derek Stephen McLean",
            "email": "delboy1978uk@gmail.com"
        }
    ],
    "require": {
        "hybridauth/hybridauth": "^3.12",
        "boneframework/bone": "^3.12",
        "delboy1978uk/bone-user": "^2.5"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "delboy1978uk/dev-tools": "^v1.1"
    },
    "autoload": {
        "psr-4": {
            "Bone\\SocialAuth\\": "src/",
            "BoneTest\\SocialAuth\\": "tests/unit"
        }
    },
    "config": {
        "allow-plugins": {
            "phpro/grumphp": false
        }
    }
}
